{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ],
      "outputLogs": "new-only"
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "env": [
        "NODE_VERSION"
      ],
      "outputs": [],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "format": {
      "outputs": []
    }
  }
}
